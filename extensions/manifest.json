{
    "manifest_version": 3,
    "name": "Privacy Policy Finder",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "extensions/background.ts"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["extensions/contentScript.ts"]
      }
    ],
    "action": {
      "default_popup": "extensions/popup.ts",
      "default_icon": "/icon.png"
    }
  }
  